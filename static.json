{
  "root": ".",
  "clean_urls": true,
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "directory_listing": false,
  "single_page_application": true
}
